{
	"name": "necord",
	"description": "A module for creating Discord bots using NestJS, based on Discord.js",
	"version": "6.0.0",
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.build.json",
		"prepublish:npm": "npm run build",
		"publish:npm": "release-it",
		"prepublish:dev": "npm run build",
		"publish:dev": "npm publish --access public --tag dev",
		"prepare": "husky install .github/husky",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ignore-path .gitignore src/**/*.ts",
		"changelog": "git cliff --prepend ./CHANGELOG.md -l --current -c ./cliff.toml -r ."
	},
	"lint-staged": {
		"*.ts": "npm run format"
	},
	"directories": {
		"lib": "src",
		"test": "test"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/necord"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [
		"nest",
		"nestjs",
		"nestjs-module",
		"discord",
		"discordjs",
		"discord bot",
		"discord bot api",
		"bot",
		"bot api",
		"bot framework"
	],
	"license": "MIT",
	"homepage": "https://necord.org",
	"repository": {
		"type": "git",
		"url": "https://github.com/necordjs/necord"
	},
	"bugs": {
		"url": "https://github.com/necordjs/necord/issues"
	},
	"author": "Alexey Filippov <socket.someone@gmail.com>",
	"contributors": [
		"Alexey Filippov <socket.someone@gmail.com>"
	],
	"dependencies": {
		"path-to-regexp": "6.2.1"
	},
	"devDependencies": {
		"@commitlint/cli": "17.7.1",
		"@commitlint/config-angular": "17.7.0",
		"@favware/npm-deprecate": "1.0.7",
		"@nestjs/common": "10.1.3",
		"@nestjs/core": "10.1.3",
		"@types/node": "20.5.0",
		"@typescript-eslint/eslint-plugin": "6.4.0",
		"@typescript-eslint/parser": "6.4.0",
		"discord-api-types": "0.37.53",
		"discord.js": "14.12.1",
		"eslint": "8.47.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.0",
		"husky": "8.0.3",
		"lint-staged": "14.0.0",
		"prettier": "3.0.2",
		"reflect-metadata": "0.1.13",
		"release-it": "16.1.5",
		"rimraf": "5.0.1",
		"rxjs": "7.8.1",
		"ts-node": "10.9.1",
		"typescript": "5.1.6"
	},
	"peerDependencies": {
		"@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
		"@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0",
		"discord.js": "^14.0.1",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.2.0"
	},
	"engines": {
		"node": ">=16.6.0",
		"npm": ">=7.0.0"
	}
}
